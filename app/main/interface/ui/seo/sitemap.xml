  <url>
    <loc>{{ request.scheme }}://{{ request.get_host }}{% url 'home' %}</loc>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>{{ request.scheme }}://{{ request.get_host }}{% url 'publicIndex' %}</loc>
    <changefreq>weekly</changefreq>
    <priority>0.9</priority>
  </url>
  <url>
    <loc>{{ request.scheme }}://{{ request.get_host }}{% url 'publicListingSoundboard' %}</loc>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
  </url>
  <url>
    <loc>{{ request.scheme }}://{{ request.get_host }}{% url 'legalNotice' %}</loc>
    <changefreq>monthly</changefreq>
    <priority>0.5</priority>
  </url>
  <url>
    <loc>{{ request.scheme }}://{{ request.get_host }}{% url 'robots_txt' %}</loc>
    <changefreq>monthly</changefreq>
    <priority>0.5</priority>
  </url>
  <url>
    <loc>{{ request.scheme }}://{{ request.get_host }}{% url 'login' %}</loc>
